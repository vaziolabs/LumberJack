{
  "$schema": "https://json.schemastore.org/tmLanguage",
  "name": "NQLT",
  "scopeName": "source.nqls",
  "fileTypes": ["nqls"],
  "patterns": [
    {
      "name": "keyword.create",
      "match": "\\bcreate\\b"
    },
    {
      "name": "keyword.network",
      "match": "\\bnetwork\\b"
    },
    {
      "name": "keyword.graph",
      "match": "\\bgraph\\b"
    },
    {
      "name": "keyword.node",
      "match": "\\bnode\\b"
    },
    {
      "name": "keyword.n",
      "match": "\\bn\\b"
    },
    {
      "name": "keyword.from",
      "match": "\\bfrom\\b"
    },
    {
      "name": "keyword.conn",
      "match": "\\bconn\\b"
    },
    {
      "name": "keyword.to",
      "match": "\\bto\\b"
    },
    {
      "name": "keyword.get",
      "match": "\\bget\\b"
    },
    {
      "name": "keyword.set",
      "match": "\\bset\\b"
    },
    {
      "name": "keyword.delete",
      "match": "\\bdelete\\b"
    },
    {
      "name": "string",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string",
      "begin": "'",
      "end": "'"
    },
    {
      "name": "entity.name.network",
      "match": "\\/"
    },
    {
      "name": "entity.name.graph",
      "match": "\"[^\"]+\""
    },
    {
      "name": "entity.name.node",
      "match": "\"[^\"]+\""
    },
    {
      "name": "entity.name.conn",
      "match": "\"[^\"]+\""
    },
    {
      "name": "constant.numeric",
      "match": "\\b\\d+\\b"
    }
  ]
}

